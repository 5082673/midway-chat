    "egg-mock": "^5.10.1",
    "jest": "^29.4.1",
    "mwts": "^1.3.0",
    "ts-jest": "^29.0.5",
    "typescript": "~4.9.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "egg_scripts_start": "egg-scripts start --daemon --midway-chat --framework=@midwayjs/web --workers=2",
    "egg_scripts_stop": "egg-scripts stop --daemon --title=midway-chat",
    "start": "NODE_ENV=production node bootstrap.js",
    "start_server": "NODE_ENV=production node server.js",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "dev_bootstrap": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts --entryFile=bootstrap.js",
    "build_dist": "midway-bin build -c && node script/build_midway.js",
    "build_dist_node_modules": "midway-bin build -c && node script/build_midway.js && cd ./build_dist && yarn install --production",
    "windows_production_start": "cross-env NODE_ENV=production node bootstrap.js",
    "windows_dev_start": "cross-env node bootstrap.js",
    "windows_dev_start_server": "cross-env node server.js",
    "dev_server": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts --entryFile=server.js",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "prune": "npm prune --production --legacy-peer-deps",
    "serve_install": "yarn install --production",
    "yarn_upd_modules": "yarn upgrade-interactive --latest",
    "typeorm_create_model_mysql": "typeorm-model-generator -h 192.168.88.130 -d momomo -p 3306 -u momomo -x a6669193 -e mysql -o mysqlModel --noConfig true --ce pascal --cp camel && node script/typeorm_create_model.js",
    "pm2_start": "NODE_ENV=production pm2 start ./bootstrap.js --name midway-chat -i 4",
    "pm2_stop": "pm2 stop midway-chat",
    "pm2_restart": "pm2 restart midway-chat"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
