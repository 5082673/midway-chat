    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/qr-image": "^3.2.5",
    "@types/qs": "^6.9.7",
