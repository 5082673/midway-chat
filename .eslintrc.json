{
  "extends": "./node_modules/mwts/",
  "ignorePatterns": ["node_modules", "dist", "test", "jest.config.js", "typings"],
  "env": {
    "jest": true
  },
  "rules": {
    "no-undef": "off",
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "tabWidth": 4
      }
    ],
    "no-case-declarations": "off",
    "no-empty": "off",
    "@typescript-eslint/ban-ts-comment": "off"
  }
}
